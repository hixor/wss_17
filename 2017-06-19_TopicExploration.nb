Cell[TextData[{
 "Notice that in general, the complete graph on ",
 StyleBox["n",
  FontSlant->"Italic"],
 " vertices requires exactly ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"n", "(", 
     RowBox[{"n", "-", "1"}], ")"}], "2"], TraditionalForm]]],
 " edges in order to connect each distinct pair of vertices by a unique edge."
}], "Text",
